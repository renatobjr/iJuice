{
  "name": "order",
  "version": "1.0.0",
  "main": "index.js",
  "nodemonConfig": {
    "watch": [
      "src/**/*"
    ],
    "ext": "ts",
    "exec": "ts-node --files -r tsconfig-paths/register ./src/index.ts"
  },
  "scripts": {
    "dev": "nodemon",
    "prisma:init": "npx prisma init",
